{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-syntax-dynamic-import",
		[
			"import",
			{
				"libraryName": "antd",
				"style": true
			},
			"antd"
		],
		[
			"import",
			{
				"libraryName": "ant-design-pro",
				"style": true
			},
			"ant-design-pro"
		]
	]
}
